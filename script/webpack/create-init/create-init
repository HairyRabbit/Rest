function NumberIndex({ value }) {
    return (Object(ink["h"])(ink["Color"], { gray: true },
        Object(ink["h"])(ink["Indent"], null,
            "[",
            value.toString(),
            "]")));
}

// CONCATENATED MODULE: ./src/components/Task.tsx
/**
 * <Task />
 */






function Task_Task({ id, title, state, result, description, during, issues, dynamic }) {
    const { color, icon, state: str } = mapToTaskStateProps(state, result);
    return (Object(ink["h"])("div", null,
        Object(ink["h"])(Icon_StateResult, { color: color, value: icon }),
        Object(ink["h"])(NumberIndex, { value: id }),
        Object(ink["h"])(Title, { value: title }),
        Object(ink["h"])(Dynamic, { value: dynamic }),
        Object(ink["h"])(During, null),
        Object(ink["h"])(ink["Indent"], null, "-"),
        Object(ink["h"])(StateResult, { color: color, value: str }),
        Object(ink["h"])(Description, null),
        Object(ink["h"])(Issues, null)));
    function Title({ value }) {
        return (Object(ink["h"])(ink["Indent"], null,
            Object(ink["h"])(ink["Bold"], null, value)));
    }
    function Dynamic({ value }) {
        if (!value)
            return null;
        return (Object(ink["h"])(ink["Color"], { magentaBright: true },
            Object(ink["h"])(ink["Indent"], null, `{dynamic}`)));
    }
    function During() {
        if (Object(lodash["isUndefined"])(during))
            return null;
        return (Object(ink["h"])(ink["Color"], { gray: true },
            Object(ink["h"])(ink["Indent"], null,
                "(",
                during,
                "ms)")));
    }
    function Description() {
        if (Object(lodash["isUndefined"])(description))
            return null;
        return (Object(ink["h"])(ink["Fragment"], null,
            ",",
            Object(ink["h"])(ink["Indent"], null, description)));
    }
    function Issues() {
        if (0 === issues.length)
            return null;
        return (Object(ink["h"])(ink["Fragment"], null, issues.map(issue => Object(ink["h"])(Issue, Object.assign({ key: `task-issue-${issue.id}` }, issue)))));
    }
    function Issue({ id, title, dynamic }) {
        return (Object(ink["h"])(ink["Fragment"], null,
            Object(ink["h"])("br", null),
            Object(ink["h"])(ink["Indent"], { key: `task-issues-${id}`, size: 2 },
                Object(ink["h"])(ink["Color"], { gray: true },
                    Object(ink["h"])(ink["Indent"], null, "|-")),
                Object(ink["h"])(NumberIndex, { value: id }),
                Object(ink["h"])(Title, { value: title }),
                Object(ink["h"])(Dynamic, { value: dynamic }))));
    }
}

// CONCATENATED MODULE: ./src/components/Tasks.tsx
/**
 * <Tasks /> tasks list views
 */


function Tasks({ value }) {
    if (0 === value.length)
        return null;
    return (Object(ink["h"])(ink["Fragment"], null, value.map(box => (Object(ink["h"])(Task_Task, { key: `task-${box.id}`, id: box.id, title: box.task.title, state: box.task.state, result: box.task.result, description: box.task.description, during: box.during, dynamic: !box.task.static, issues: mapToTaskIssues(box.issues) })))));
}
function mapToTaskIssues(issues) {
    return Array.from(issues).map(issue => ({
        id: issue.id,
        title: issue.task.title,
        dynamic: !issue.task.static
    }));
}

// CONCATENATED MODULE: ./src/components/ErrorList.tsx
/**
 * <ErrorList />
 */


function ErrorList({ value }) {
    if (0 === value.length)
        return null;
    return (Object(ink["h"])(ink["Fragment"], null, value.map(({ box, error }) => (Object(ink["h"])(ink["Fragment"], null,
        Object(ink["h"])("div", { key: box.id },
            Object(ink["h"])(NumberIndex, { value: box.id }),
            Object(ink["h"])(ink["Color"], { redBright: true }, box.task.title)),
        Object(ink["h"])("br", null),
        Object(ink["h"])("div", null,
            Object(ink["h"])(ink["Indent"], { size: 5 }, error.stack)))))));
}

// CONCATENATED MODULE: ./src/components/Manager.tsx
/**
 * <Manager /> task manager view
 */







function Manager({ value }) {
    const { icon, color, state } = mapToTaskManagerStateProps(value.state, value.result);
    return (Object(ink["h"])(ink["Fragment"], null,
        Object(ink["h"])(Header, null),
        Object(ink["h"])(Body, null),
        Object(ink["h"])(Footer, null)));
    function Header() {
        return (Object(ink["h"])(Box, null,
            Object(ink["h"])(ink["Indent"], null,
                Object(ink["h"])(Icon_StateResult, { color: color, value: icon }),
                Object(ink["h"])(StateResult, { color: color, value: state }),
                Object(ink["h"])(ink["Color"], { grey: true },
                    Object(ink["h"])(ink["Indent"], null,
                        "(",
                        value.tasks.size,
                        ")")))));
    }
    function Body() {
        return (Object(ink["h"])(Box, null,
            Object(ink["h"])(ink["Indent"], { size: 3 },
                Object(ink["h"])(Tasks, { value: Array.from(value.tasks.values()) }))));
    }
    function Footer() {
        return (Object(ink["h"])(Box, null,
            Object(ink["h"])(ink["Indent"], null,
                Object(ink["h"])(ErrorList, { value: Array.from(value.errors) }))));
    }
}

// CONCATENATED MODULE: ./src/components/index.tsx
