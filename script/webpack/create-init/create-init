// CONCATENATED MODULE: ./src/utils/hash.ts
/**
 * get file hash if exists
 */


function getFileHash(filepath, algorithm, encoding) {
    if (!filepath)
        return null;
    if (!external_fs_["existsSync"](filepath))
        return null;
    const stat = external_fs_["statSync"](filepath);
    if (!stat.isFile)
        return null;
    const content = external_fs_["readFileSync"](filepath);
    return getContentHash(content, algorithm, encoding);
}
function getContentHash(content, algorithm = 'md5', encoding = 'hex') {
    return external_crypto_["createHash"](algorithm)
        .update(content)
        .digest(encoding);
}

// CONCATENATED MODULE: ./src/utils/mixins.ts
function applyMixins(derivedCtor, ...baseCtors) {
    baseCtors.forEach(baseCtor => {
        Object.getOwnPropertyNames(baseCtor.prototype).forEach(name => {
            derivedCtor.prototype[name] = baseCtor.prototype[name];
        });
    });
    return derivedCtor;
}

// CONCATENATED MODULE: ./src/utils/filepath.ts
/**
 * fs and path utils
 */


function isTargetExists(f) {
    return (target) => {
        if (!external_fs_["existsSync"](target))
            return false;
        try {
            const stats = external_fs_["statSync"](target);
            if (f(stats))
                return true;
            return false;
        }
        catch (e) {
            return false;
        }
    };
}
const isDirExists = isTargetExists((stats) => stats.isDirectory());
const isFileExists = isTargetExists((stats) => stats.isFile());
function getParentPath(target) {
    const p = external_path_["resolve"](target).split(external_path_["sep"]);
    p.pop();
    return p.join(external_path_["sep"]);
}
function formatToPosixPath(target) {
    return target.split(external_path_["sep"]).join(external_path_["posix"].sep);
}
function isPathRelativeContext(filepath, context) {
    const relative = external_path_["relative"](context, filepath);
    return !(relative.startsWith('..') || relative === filepath);
}
function listDirs(dirpath, context) {
    return external_path_["relative"](context, external_path_["normalize"](dirpath))
        .split(external_path_["sep"])
        .reduce((acc, curr) => {
        acc.push(acc.length ? external_path_["join"](acc[acc.length - 1], curr) : curr);
        return acc;
    }, [])
        .map(p => external_path_["join"](context, p));
}
function rmdirRecur(dirpath, context) {
    return rmdirs(listDirs(dirpath, context).reverse());
}
function rmdirs(dirpaths) {
    return dirpaths.forEach(external_fs_["rmdirSync"].bind(external_fs_));
}
function checkDirExists(dirpath, context) {
    const dirs = listDirs(dirpath, context);
    let dir;
    while ((dir = dirs.shift())) {
        if (!external_fs_["existsSync"](dir)) {
            dirs.unshift(dir);
            break;
        }
    }
    return dirs;
}

// CONCATENATED MODULE: ./src/utils/async.ts
/**
 * async tools
 */
async function forEachParallel(f, list) {
    await Promise.all(list.map(async (x, i, l) => await f(x, i, l)));
}
function forEachMapParallel(f, map) {
    return forEachParallel(f, Array.from(map.values()));
}
async function forEachSeries(f, list) {
    let i = 0;
    for await (const item of list) {
        f(item, i, list);
        i++;
    }
}

