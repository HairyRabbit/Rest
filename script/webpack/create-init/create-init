// CONCATENATED MODULE: ./src/modules/mkdir.ts
/**
 * create dir
 */




class mkdir_MakeDir extends Task {
    /**
     * initial and validate options
     */
    constructor(context, options) {
        super(context);
        this.context = context;
        this.options = options;
        this.flag = '<mkdir>';
        this.exists = false;
        this.created = false;
        const { root } = context;
        const { dirpath } = options;
        if (!dirpath) {
            throw new Error(`${this.flag} options.dirpath was required`);
        }
        /**
         * make sure path was absoulte
         */
        this.target = external_path_default.a.resolve(dirpath);
        const relative = external_path_default.a.relative(root, this.target);
        this.id = `mkdir(${this.target})`;
        this.title = `create dir @/${root === this.target ? '' : formatToPosixPath(relative) + '/'}`;
    }
    async validate() {
        this.exists = isDirExists(this.target);
        if (this.exists)
            return;
        /**
         * if parent path not exists, create it as a dependency task
         */
        const parent = getParentPath(this.target);
        if (isDirExists(parent))
            return;
        this.dependencies.add(new mkdir_MakeDir(this.context, { dirpath: parent }));
    }
    async run() {
        if (this.exists)
            return TaskResult.Skip;
        external_fs_default.a.mkdirSync(this.target, { recursive: true });
        this.created = true;
    }
    async rollback() {
        if (!this.created)
            return TaskResult.Skip;
        external_fs_default.a.rmdirSync(this.target);
    }
}

// CONCATENATED MODULE: ./src/modules/mkfile.ts
/**
 * create file from content
 */






class mkfile_MakeFile extends Task {
    constructor(context, options) {
        super(context);
        this.context = context;
        this.options = options;
        this.flag = '<mkfile>';
        this.exists = false;
        this.sumchecked = false;
        this.created = false;
        const { root } = context;
        const { _: defaults = [], filepath: _filepath, content: _content, override = false } = this.options;
        const [_filepath_, _content_] = defaults;
        const filepath = _filepath || _filepath_;
        const content = _content || _content_;
        if (Object(lodash["isUndefined"])(filepath))
            throw new Error(`${this.flag} file path was required`);
        this.target = external_path_default.a.resolve(root, filepath);
        const relative = external_path_default.a.relative(root, this.target);
        this.content = content || '';
        this.id = `mkfile(${this.target})`;
        this.title = `create file @/${formatToPosixPath(relative)}`;
        this.override = override;
    }
    validate() {
        /**
         * check file and dir paths
         */
        this.exists = isFileExists(this.target);
        if (!this.exists) {
            const dir = external_path_default.a.dirname(this.target);
            /**
             * if dir not exists, add sub task to create dir first
             */
            if (isDirExists(dir))
                return;
            this.dependencies.add(new mkdir_MakeDir(this.context, { dirpath: dir }));
            return;
        }
        /**
         * checksum use md5
         */
        const filehash = getFileHash(this.target);
        const contenthash = getContentHash(this.content);
        this.sumchecked = filehash === contenthash;
        /**
         * throw when checksum failed, and override not allowed
         */
        if (false === this.override && false === this.sumchecked)
            throw new Error(`<mkfile> file already exists and invalid ${this.target}`);
    }
    run() {
        /**
         * skipped task, when the sumchecked
         */
        if (this.sumchecked)
            return TaskResult.Skip;
        external_fs_default.a.writeFileSync(this.target, this.content, 'utf8');
        this.created = true;
    }
    rollback() {
        if (!this.created)
            return TaskResult.Skip;
        external_fs_default.a.unlinkSync(this.target);
    }
}

// CONCATENATED MODULE: ./src/modules/gitignore/index.ts
/**
 * create .gitignore file
 */



class gitignore_GitIgnore extends mkfile_MakeFile {
    constructor(context, options) {
        super(context, { filepath: '.gitignore', ...options });
        this.context = context;
        this.options = options;
        this.id = `gitignore`;
        this.content = Object(lodash["template"])(template_default.a)({ addons: options.addons || '' });
        this.flag = `<${this.id}>`;
    }
}

