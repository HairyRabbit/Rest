function Icon_StateResult({ color, value }) {
    return (Object(ink["h"])(ink["Color"], Object.assign({}, { [color]: true }), 'boolean' === typeof value
        ? (value ? Object(ink["h"])(dist_default.a, null) : null)
        : value));
}

// CONCATENATED MODULE: ./src/components/Number.tsx
