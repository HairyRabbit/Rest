/***/ (function(module, exports, __webpack_require__) {

"use strict";

var shebangRegex = __webpack_require__(131);
module.exports = function (str) {
    var match = str.match(shebangRegex);
    if (!match) {
        return null;
    }
    var arr = match[0].replace(/#! ?/, '').split(' ');
    var bin = arr[0].split('/').pop();
    var arg = arr[1];
    return (bin === 'env' ?
        arg :
        bin + (arg ? ' ' + arg : ''));
};


