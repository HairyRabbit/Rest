// CONCATENATED MODULE: ./src/modules/group.ts
/**
 * group tasks
 */

class group_Group extends Task {
    constructor(context, options) {
        super(context);
        this.context = context;
        this.options = options;
        this.flag = '<group>';
        const { _: defaults = [], name: _name } = this.options;
        const [_name_] = defaults;
        const name = _name || _name_;
        if (!name)
            throw new Error(`${this.flag} group name was required`);
        this.name = name;
        this.id = `group(${this.name})`;
        this.title = `+${this.name}`;
    }
    run() { return TaskResult.Skip; }
}

