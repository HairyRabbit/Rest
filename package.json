{
  "private": true,
  "name": "Rest",
  "version": "0.0.1",
  "description": "The Rest app framework & scaffold",
  "main": "index.js",
  "scripts": {
    "test": "npx cross-env NODE_ENV=test npx mocha -r @babel/register -r jsdom-global/register -r esm -r script/mock/index.js component util script --recursive",
    "test:debug": "npx cross-env NODE_ENV=test npx mocha -r @babel/register -r jsdom-global/register -r esm -r script/mock/index.js component util script --recursive",
    "type": "npx flow check",
    "test:website": "npx cross-env NODE_ENV=test npx mocha -r @babel/register -r jsdom-global/register -r esm -r script/mock/index.js website --recursive",
    "prebuild:website": "rm -rf docs/*",
    "start:website": "npx cross-env NODE_ENV=development npx webpack-dev-server -r @babel/register -r esm --config website/webpack.config.js",
    "build:website": "npx cross-env NODE_ENV=production npx webpack -r @babel/register -r esm --config website/webpack.config.js",
    "postbuild:website": "cp CNAME docs/ & cp docs/index.html docs/404.html & touch docs/.nojekyll",
    "prestart:scaffold": "npx cross-env NODE_ENV=development npx webpack -r @babel/register -r esm --config scaffold/webpack.config.js",
    "start:scaffold": "start npx cross-env NODE_ENV=development npx electron ./scaffold/build && start npx webpack-dev-server -r @babel/register -r esm --config scaffold/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HairyRabbit/Rest.git"
  },
